{
  "name": "publish",
  "version": "1.0.0",
  "description": "DADI Publish",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development node index.js",
    "test": "standard --fix && env NODE_ENV=development mocha --colors --webpack-config webpack.config.js \"test/**/*.js\"",
    "build": "webpack --progress",
    "prebuild": "mkdirp build",
    "init": "validate-commit-msg",
    "create-component": "node scripts/create-component/create-component.js",
    "watch": "NODE_ENV=development npm-run-all --parallel watch:server watch:build",
    "watch:build": "webpack --watch",
    "watch:server": "nodemon \"./index.js\" --watch \"./app\"",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "build-docs": "rm -rf docs/components/* && react-docgen --exclude ['/^(ConfigSchema$)/g'] frontend/components | node scripts/build-docs/build-docs.js docs/components; rm -rf docs/containers/* && react-docgen --exclude ['/^(ConfigSchema$)/g'] frontend/containers | node scripts/build-docs/build-docs.js docs/containers"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dadi/publish.git"
  },
  "keywords": [
    "DADI",
    "Publish",
    "Publishing"
  ],
  "author": "DADI",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dadi/publish/issues"
  },
  "engines": {
    "node": ">=4.7",
    "npm": ">=3.0"
  },
  "homepage": "https://github.com/dadi/publish#readme",
  "dependencies": {
    "@dadi/api-wrapper": "^1.4.0",
    "@dadi/api-wrapper-core": "^1.1.1",
    "@dadi/logger": "^1.1.4",
    "@dadi/passport": "^1.2.0",
    "aws-sdk": "^2.19.0",
    "connect-flash": "^0.1.1",
    "convict": "^1.5.0",
    "cookie-session": "^2.0.0-alpha.2",
    "history": "^4.5.1",
    "md5": "^2.2.1",
    "mkdirp": "^0.5.1",
    "passport-local": "^1.0.0",
    "passport-restify": "^1.0.1",
    "passport-strategy": "^1.0.0",
    "path": "^0.12.7",
    "preact-router": "github:eduardoboucas/preact-router",
    "request-promise": "^4.1.1",
    "restify": "^4.3.0",
    "restify-cookies": "^0.2.2",
    "restify-errors": "^4.3.0",
    "serve-static-restify": "^1.0.0",
    "snyk": "^1.25.0",
    "socketcluster-server": "^5.3.0",
    "url": "^0.11.0",
    "ws": "2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.3",
    "babel": "^6.23.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-template-literals": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fs": "0.0.1-security",
    "inquirer": "^3.0.6",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "normalize.css": "^5.0.0",
    "npm-run-all": "^3.1.2",
    "on-build-webpack": "^0.1.0",
    "postcss": "^5.2.14",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-loader": "^1.3.0",
    "pre-commit": "^1.2.2",
    "preact": "^6.4.0",
    "preact-compat": "^3.0.0",
    "preact-redux": "^1.2.0",
    "preact-router-redux": "^4.1.0",
    "proptypes": "^0.14.3",
    "react-docgen": "github:eduardoboucas/react-docgen",
    "redux": "^3.6.0",
    "socketcluster-client": "^5.1.1",
    "standard": "^8.6.0",
    "style-loader": "^0.13.1",
    "uglify-js": "github:mishoo/UglifyJS2#harmony",
    "uglifyjs-webpack-plugin": "^0.3.0",
    "unfetch": "^2.1.1",
    "url-loader": "^0.5.7",
    "validate-commit-msg": "^2.11.1",
    "validator": "^7.0.0",
    "webpack": "^1.14.0"
  },
  "standard": {
    "globals": [
      "paths"
    ],
    "fix": true,
    "ignore": [
      "build",
      ".wallet",
      "config",
      "docs",
      "public",
      "test",
      "frontend"
    ]
  },
  "pre-commit": [
    "build-docs",
    "test"
  ],
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "snyk": true
}
