{
  "name": "@dadi/publish",
  "version": "1.0.11-beta",
  "description": "DADI Publish",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "profile": "webpack --progress --profile --json > stats.json",
    "build-docs": "rm -rf docs/components/* && react-docgen --exclude ['/^(ConfigSchema$)/g'] frontend/components | node scripts/build-docs/build-docs.js docs/components; rm -rf docs/containers/* && react-docgen --exclude ['/^(ConfigSchema$)/g'] frontend/containers | node scripts/build-docs/build-docs.js docs/containers; cat component-map.json | node scripts/build-component-tree/build-tree.js docs/component-architecture.md",
    "create-component": "node scripts/create-component/create-component.js",
    "create-user": "node scripts/create-user/create-user.js",
    "api-install": "node scripts/api-install/api-install.js",
    "lint": "eslint frontend && standard --fix",
    "prepublishOnly": "npm run snyk-protect && npm run build",
    "prepush": "npm run build-docs",
    "precommit": "npm run lint && node scripts/precommit.js",
    "snyk-protect": "snyk protect",
    "start": "NODE_ENV=development node index.js",
    "test": "npm-run-all test-all lint",
    "test-watch": "jest --watch",
    "test-all": "jest --coverage && node scripts/coverage/coverage.js",
    "watch": "NODE_ENV=development npm-run-all --parallel watch:server watch:build",
    "watch:build": "webpack --watch",
    "watch:server": "nodemon \"./index.js\" --watch \"./app\"",
    "storybook:run": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o public/storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dadi/publish.git"
  },
  "keywords": [
    "DADI",
    "Publish",
    "Publishing"
  ],
  "author": "DADI <team@dadi.tech>",
  "license": "SEE LICENSE IN GPL.md",
  "bugs": {
    "url": "https://github.com/dadi/publish/issues"
  },
  "engines": {
    "node": ">=4.7",
    "npm": ">=3.0"
  },
  "homepage": "https://github.com/dadi/publish",
  "dependencies": {
    "@dadi/api-wrapper": "^3.2.0",
    "@dadi/boot": "^1.1.4",
    "@dadi/logger": "^1.4.1",
    "@dadi/ssl": "^1.1.0",
    "connect-flash": "^0.1.1",
    "convict": "^1.5.0",
    "cookie-session": "^2.0.0-alpha.2",
    "md5": "^2.2.1",
    "restify": "^4.3.0",
    "restify-cookies": "^0.2.2",
    "restify-errors": "^4.3.0",
    "socketcluster-client": "^5.3.0",
    "socketcluster-server": "^5.3.0",
    "ws": "2.0.0"
  },
  "devDependencies": {
    "@dadi/api-wrapper-core": "^3.1.0",
    "@dadi/preact-router": "^2.6.2",
    "@storybook/react": "^3.4.10",
    "archy": "^1.0.0",
    "autoprefixer": "^6.7.3",
    "babel-eslint": "^7.2.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-minify": "^0.2.0",
    "babel-plugin-minify-mangle-names": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-template-literals": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.18.0",
    "babili-webpack-plugin": "^0.1.1",
    "chai": "^3.5.0",
    "colors": "^1.3.1",
    "component-tree-webpack-plugin": "^1.0.0",
    "coveralls": "^3.0.2",
    "css-loader": "^0.26.0",
    "eslint": "^3.18.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "fecha": "^2.3.0",
    "file-loader": "^0.9.0",
    "file-size": "^1.0.0",
    "fs": "0.0.1-security",
    "history": "^4.5.1",
    "html-looks-like": "^1.0.3",
    "husky": "^0.13.3",
    "inquirer": "^3.0.6",
    "jest": "^20.0.4",
    "js-cookie": "^2.2.0",
    "marked": "^0.5.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "nock": "^9.6.1",
    "node-mocks-http": "^1.6.3",
    "normalize.css": "^5.0.0",
    "npm-run-all": "^3.1.2",
    "on-build-webpack": "^0.1.0",
    "path": "^0.12.7",
    "pell": "^1.0.4",
    "postcss": "^5.2.14",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-loader": "^1.3.0",
    "pre-build-webpack": "^0.1.0",
    "preact": "^8.3.1",
    "preact-compat": "^3.18.3",
    "preact-jsx-chai": "^2.2.1",
    "preact-jsx-chai-match-template": "^0.2.2",
    "preact-redux": "^1.2.0",
    "preact-render-to-string": "^3.8.2",
    "preact-router-redux": "^4.1.0",
    "preact-svg-loader": "^0.2.1",
    "proptypes": "^0.14.3",
    "react-docgen": "^2.21.0",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "request-promise": "^4.1.1",
    "rollup": "^0.47.4",
    "snyk": "^1.92.6",
    "standard": "^8.6.0",
    "style-loader": "^0.13.1",
    "turndown": "^5.0.1",
    "unfetch": "^2.1.1",
    "url-loader": "^0.5.7",
    "validator": "^7.0.0",
    "webpack": "^3.6.0"
  },
  "standard": {
    "globals": [
      "paths"
    ],
    "fix": true,
    "ignore": [
      "build",
      ".wallet",
      "config",
      "docs",
      "public",
      "test",
      "scripts",
      "frontend",
      "stories"
    ]
  },
  "jest": {
    "verbose": true,
    "collectCoverage": false,
    "automock": false,
    "collectCoverageFrom": [
      "**/frontend/**/*.{js,jsx}",
      "**/app/**/*.{js}",
      "!**/frontend/**/Mock*.{js,jsx}"
    ],
    "coverageReporters": [
      "cobertura",
      "text"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^actions/(.*)": "<rootDir>/frontend/actions/$1",
      "^components/(.*)": "<rootDir>/frontend/components/$1",
      "^containers/(.*)": "<rootDir>/frontend/containers/$1",
      "^views/(.*)": "<rootDir>/frontend/views/$1",
      "^reducers/(.*)": "<rootDir>/frontend/reducers/$1",
      "^frontend/components/(.*)": "<rootDir>/frontend/components/$1",
      "^lib/(.*)": "<rootDir>/frontend/lib/$1",
      "^middleware/(.*)": "<rootDir>/frontend/middleware/$1",
      "\\.css$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat",
      "request-promise": "<rootDir>/test/mocks/request-promise",
      "fetch": "unfetch/polyfill",
      "restify": "<rootDir>/test/mocks/restify",
      "@dadi/ssl": "<rootDir>/test/mocks/ssl",
      "@dadi/api-wrapper": "<rootDir>/test/mocks/api-wrapper"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "setupFiles": [
      "./test/jestSetup.js"
    ],
    "testMatch": [
      "**/frontend/**/*.test.jsx",
      "**/test/**/*.test.js"
    ],
    "testURL": "http://localhost"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "snyk": true
}
